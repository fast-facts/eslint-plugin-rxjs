{
  "author": "Nicholas Jamieson <nicholas@cartant.com>",
  "bugs": {
    "url": "https://github.com/cartant/eslint-plugin-rxjs/issues"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^8.0.0",
    "common-tags": "^1.8.0",
    "decamelize": "^5.0.0",
    "requireindex": "~1.2.0",
    "rxjs-report-usage": "^1.0.4",
    "tslib": "^2.0.0",
    "tsutils": "^3.0.0",
    "tsutils-etc": "^1.4.1"
  },
  "description": "ESLint rules for RxJS",
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.0",
    "@eslint/js": "^9.0.0",
    "@types/chai": "^4.2.0",
    "@types/common-tags": "^1.8.0",
    "@types/eslint": "^9.0.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@typescript-eslint/rule-tester": "^8.0.0",
    "chai": "^4.2.0",
    "copyfiles": "^2.0.0",
    "eslint": "^9.0.0",
    "lint-staged": "^13.0.0",
    "mocha": "^9.0.0",
    "prettier": "^2.0.0",
    "rxjs": "^7.0.0",
    "ts-node": "^10.0.0",
    "typescript": "~4.7.4",
    "typescript-eslint": "^8.0.0"
  },
  "files": [
    "dist",
    "docs"
  ],
  "homepage": "https://github.com/fast-facts/eslint-plugin-rxjs",
  "keywords": [
    "lint",
    "rules",
    "eslint",
    "rxjs"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts}": "prettier --write"
  },
  "main": "./index.js",
  "name": "eslint-plugin-rxjs-updated",
  "optionalDependencies": {},
  "peerDependencies": {
    "eslint": "^9.0.0",
    "typescript": ">=4.0.0"
  },
  "private": false,
  "publishConfig": {
    "tag": "latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fast-facts/eslint-plugin-rxjs.git"
  },
  "scripts": {
    "predist": "rm -rf dist",
    "dist": "yarn run lint && yarn run dist:build",
    "dist:build": "tsc -p tsconfig.dist.json",
    "postdist": "yarn run copy:files",
    "copy:files": "copyfiles package.json README.md LICENSE CHANGELOG.md dist",
    "lint": "eslint",
    "prettier": "prettier --write \"./{src,tests}/**/*.{js,json,ts,tsx}\"",
    "prettier:ci": "prettier --check \"./{src,tests}/**/*.{js,json,ts,tsx}\"",
    "test": "yarn run test:rules && yarn run test:specs",
    "test:debug": "mocha -r ts-node/register -t 5000 tests/rules/no-explicit-generics.ts --watch-files ./**/*.ts --watch",
    "test:rules": "mocha -r ts-node/register -t 5000 tests/rules/*.ts",
    "test:specs": "mocha -r ts-node/register tests/*-spec.ts"
  },
  "version": "0.0.0-semantic-release"
}