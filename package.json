{
  "author": "Nicholas Jamieson <nicholas@cartant.com>",
  "bugs": {
    "url": "https://github.com/cartant/eslint-plugin-rxjs/issues"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^8.6.0",
    "common-tags": "^1.8.2",
    "decamelize": "^5.0.1",
    "requireindex": "~1.2.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.7.0",
    "tsutils": "^3.21.0",
    "tsutils-etc": "^1.4.2"
  },
  "description": "ESLint rules for RxJS",
  "devDependencies": {
    "@types/chai": "^4.3.19",
    "@types/common-tags": "^1.8.4",
    "@types/eslint": "^9.6.1",
    "@types/mocha": "^10.0.8",
    "@types/node": "^22.5.5",
    "@typescript-eslint/parser": "^8.6.0",
    "@typescript-eslint/rule-tester": "^8.6.0",
    "chai": "^5.1.1",
    "eslint": "^9.11.0",
    "mocha": "^10.7.3",
    "rxjs": "^7.8.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.6.0"
  },
  "files": [
    "dist",
    "docs"
  ],
  "homepage": "https://github.com/fast-facts/eslint-plugin-rxjs",
  "keywords": [
    "lint",
    "rules",
    "eslint",
    "rxjs"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "name": "eslint-plugin-rxjs-updated",
  "peerDependencies": {
    "eslint": "^9.0.0",
    "typescript": ">=4.0.0"
  },
  "private": false,
  "publishConfig": {
    "tag": "latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fast-facts/eslint-plugin-rxjs.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.dist.json",
    "test": "npm run test:rules",
    "test:debug": "mocha -r ts-node/register -t 5000 tests/rules/prefer-async-pipe.ts --watch-files ./**/*.ts --watch",
    "test:rules": "mocha -r ts-node/register -t 5000 tests/rules/*.ts",
    "test:specs": "mocha -r ts-node/register tests/*-spec.ts"
  },
  "version": "0.0.0-semantic-release"
}